   "source": [
    "# Example visualization of NetworkxD3 package.\n",
    "# Author - Janu Verma\n",
    "# jv367@cornell.edu\n",
    "# @januverma\n",
    "\n",
    "import networkx as nx\n",
    "from NetworkxD3 import simpleNetworkx\n",
    "\n",
    "G = nx.Graph()\n",
    "\n",
    "H = [\"A\",\"B\",\"C\",\"D\",\"E\",\"F\",\"G\", \"H\",\"I\",\"J\"]\n",
    "\n",
    "G.add_nodes_from(H)\n",
    "\n",
    "G.add_edges_from([(\"A\",\"B\"), (\"A\",\"C\"), (\"A\",\"D\"), (\"A\",\"J\"), (\"B\",\"E\"), (\"B\",\"F\"),\n",
    "(\"C\",\"G\"),(\"C\",\"H\"), (\"D\",\"I\")])\n",
    "\n",
    "simpleNetworkx(G)\n"
   ]
  },
